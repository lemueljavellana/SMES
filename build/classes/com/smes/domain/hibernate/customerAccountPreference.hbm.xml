<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.smes.domain.hibernate.CustomerAccountPreference">
		<id name="customerAccountPreferenceId" column="CUSTOMER_ACCOUNT_PREFERENCE_ID" >
			<generator class="assigned" ></generator>
		</id>
		<property name="customerId">
			<column name="CUSTOMER_ID"/>
		</property>
		<property name="interest">
			<column name="INTEREST"/>
		</property>
		<property name="createdBy">
			<column name="CREATED_BY" />
		</property>
		<property name="createdDate">
			<column name="CREATED_DATE" />
		</property>
		<property name="modifiedBy">
			<column name="MODIFIED_BY" />
		</property>
		<property name="modifiedDate">
			<column name="MODIFIED_DATE" />
		</property>
		<many-to-one name="customer" update="false" insert="false"
			class="com.smes.domain.hibernate.Customer" column="CUSTOMER_ID" lazy="false"/>			 
	</class>
</hibernate-mapping>